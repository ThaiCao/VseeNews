<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vsee.news">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VseeNews">
        <activity
            android:name=".presentation.features.news.activity.FeedNewsActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.VseeNews.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".presentation.features.newdetail.activity.FeedDetailActivity"
            android:theme="@style/Theme.VseeNews.NoActionBar"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>